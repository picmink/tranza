<%= search_form_for @search do |f| %>
  <%= f.search_field :name_cont, placeholder: '検索ワード' %>
  <%= f.submit '検索' %>
<% end %>

<% @users.each do |user| %>
  <table>
    <%= link_to image_tag(user.get_profile_image(100, 100)), user_path(user) %>
    <%= link_to user.name, user_path(user) %>
  </table>
<% end %>
<%= paginate @users %>